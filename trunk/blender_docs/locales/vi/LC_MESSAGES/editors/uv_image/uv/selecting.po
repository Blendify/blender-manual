#: ../../manual/editors/uv_image/uv/selecting.rst:58
msgid "Select individual vertices."
msgstr ""

#: ../../manual/editors/uv_image/uv/selecting.rst:59
#: ../../manual/editors/uv_image/uv/selecting.rst:85
msgid "Edge"
msgstr ""

#: ../../manual/editors/uv_image/uv/selecting.rst:60
msgid "Select edges."
msgstr ""

