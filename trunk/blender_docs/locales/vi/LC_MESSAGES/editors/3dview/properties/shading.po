msgstr ""

#: ../../manual/editors/3dview/properties/shading.rst:56
msgid "Switches between the current and *Rendered* draw modes."
msgstr ""

#: ../../manual/editors/3dview/properties/shading.rst:57
msgid ":kbd:`Shift-Z`"
msgstr ""

#: ../../manual/editors/3dview/properties/shading.rst:58
msgid "Switches between *Solid* and *Textured* draw modes."
msgstr ""

