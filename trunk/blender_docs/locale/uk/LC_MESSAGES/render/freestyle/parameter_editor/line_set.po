#: ../../manual/render/freestyle/parameter_editor/line_set.rst:240
msgid "Inclusive/Exclusive"
msgstr ""

#: ../../manual/render/freestyle/parameter_editor/line_set.rst:200
msgid ""
"Whether to include or exclude edges matching defined face mark conditions"
" from the line set."
msgstr ""

#: ../../manual/render/freestyle/parameter_editor/line_set.rst:202
msgid "One Face"
msgstr ""

#: ../../manual/render/freestyle/parameter_editor/line_set.rst:203
msgid "(De)select all edges which have one or both neighbor faces marked."
msgstr ""

#: ../../manual/render/freestyle/parameter_editor/line_set.rst:205
msgid "Both Faces"
msgstr ""

#: ../../manual/render/freestyle/parameter_editor/line_set.rst:205
msgid "(De)select all edges which have both of their neighbor faces marked."
msgstr ""

